<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

<!-- autowire-candidate = "false" means that the bean will not participate in autowiring -->
    <bean class="in.sp.beans.Address" id = "addrId1" autowire-candidate="false">
        <property name="houseno" value="897"/>
        <property name="city" value="Kathmandu"/>
        <property name="pincode" value="44600"/>
    </bean>

    <bean class="in.sp.beans.Address" id = "addrId2">
        <property name = "houseno" value="268"/>
        <property name="city" value="Rolpa"/>
        <property name="pincode" value="33600"/>
    </bean>

<!--    by type checks the data type and perfroms operations -->
    <bean class="in.sp.beans.Student" id = "stdId" autowire="byType">
        <property name="name" value="Nabin Oli"/>
        <property name="rollno" value="19"/>
<!--        <property name="address" ref="addrId"/>-->
    </bean>
</beans>